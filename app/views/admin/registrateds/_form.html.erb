<%= form_for([:admin,@registrated]) do |f| %>

	<%= render 'shared/error_messages', object: f.object %>
	<%= f.label :title %>
	<%= f.text_field :title, class: 'form-control' %>
	<%= f.label :place %>
	<%= f.text_field :place, class: 'form-control' %>
	<%= f.label :time_registrated %>
	<%= f.date_field :time_registrated, class: 'form-control' %>
	Các Loại xe Cho Phép:
	<%= collection_check_boxes(:registrated, :car_ids, Car.all, :id, :name) do |b| %>
   	<%= b.label(:"data-value" => b.value) { b.check_box + b.text } %>
<% end %>
	<br /><%= f.submit yield(:button_text), class: "btn btn-primary" %>	
<% end %>